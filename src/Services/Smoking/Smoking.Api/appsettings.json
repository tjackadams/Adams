{
  "ConnectionStrings": {
    "Smoking": "Server=localhost;Database=Adams.Service.SmokingDb;Integrated Security=true;MultipleActiveResultSets=true",
    "redis": "localhost:6379"
  },
  "IdentityUrl": "https://localhost:5105",
  "Serilog": {
    "SeqServerUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Authentication": "Debug"
      }
    },
    "Using": [ "Serilog.Expressions" ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath in ['/hc', '/liveness']"
        }
      }
    ]
  },
  "OpenTelemetry": {
    "ExcludedPaths": [ "/hc", "/liveness" ],
    "Zipkin": {
      "ServerUrl":  "http://localhost:9411" 
    } 
  } 
}