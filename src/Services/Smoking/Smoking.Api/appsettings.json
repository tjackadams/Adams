{
  "ConnectionStrings": {
    "Smoking": "Server=localhost;Database=Adams.Service.SmokingDb;Integrated Security=true;MultipleActiveResultSets=true",
    "redis": "localhost:6379"
  },
  "IdentityUrl": "https://localhost:5105",
  "Serilog": {
    "SeqServerUrl": null,
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Using": [ "Serilog.Expressions" ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath in ['/hc', '/liveness']"
        }
      }
    ]
  }
}