{
  "TokenLifetimeMinutes": 120,
  "PermanentTokenLifetimeDays": 365,
  "IsClusterEnv": "False",
  "BlazorClient": "https://localhost:5100",
  "SmokingApiClient": "https://localhost:5102",
  "ConnectionStrings": {
    "Identity": "Server=localhost;Database=Adams.Service.IdentityDb;Integrated Security=true;MultipleActiveResultSets=true",
    "redis": "localhost:6379"
  },
  "AzureKeyVault": {
    "VaultUri": "",
    "CertificateName": ""
  },
  "Serilog": {
    "SeqServerUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Using": [ "Serilog.Expressions" ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath in ['/hc', '/liveness']"
        }
      }
    ]
  },
  "OpenTelemetry": {
    "ExcludedPaths": [ "/hc", "/liveness" ],
    "Zipkin": {
      "ServerUrl": "http://localhost:9411"
    }
  }
}