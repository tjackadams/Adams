@inherits BlazorStateComponent

@if (CurrentClient is not null && ChartData is not null)
{
    <MudGrid>

        <MudItem xs="12">
            <MudChart ChartType="ChartType.Line"
                      ChartSeries="ChartData.Series.ToList()"
                      @bind-SelectedIndex="Index"
                      XAxisLabels="ChartData.XAxisLabels.ToArray()"
                      Width="100%"
                      Height="350px">
            </MudChart>
        </MudItem>

    </MudGrid>
}